{
  "name": "Live HTTP headers",
  "description": "Displays the live log with the http requests headers",
  "version": "0.7",
  "permissions": [
    "debugger",
    "http://127.0.0.1:8000/*",
    "tabs"
  ],
  "background": {
    "scripts": ["popup.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Live HTTP headers"
  },
  "manifest_version": 2
}
